{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "essential": true,
            "image": "",
            "name": "web-scraper",
            "memoryReservation": 1700,
            "logConfiguration": {
                "logDriver": "json-file",
                "options": {
                    "max-size": "200m",
                    "max-file": "10"
                }
            },
            "mountPoints": [
                {
                    "containerPath": "/usr/src/app/logs",
                    "sourceVolume": "AppLog"
                },
                {
                    "containerPath": "/tmp",
                    "sourceVolume": "Tmp"
                }
            ],
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 80
                }
            ]
        },
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "/proc"
            },
            "name": "Proc"
        },
        {
            "host": {
                "sourcePath": "/sys"
            },
            "name": "Sys"
        },
        {
            "host": {
                "sourcePath": "/run/dbus/system_bus_socket"
            },
            "name": "RunDbusSystem_Bus_Socket"
        },
        {
            "host": {
                "sourcePath": "/var/run"
            },
            "name": "VarRun"
        },
        {
            "host": {
                "sourcePath": "/var/lib/docker"
            },
            "name": "VarLibDocker"
        },
        {
            "host": {
                "sourcePath": "/cgroup"
            },
            "name": "Cgroup"
        },
        {
            "host": {
                "sourcePath": "/"
            },
            "name": "RootFs"
        },
        {
            "host": {
                "sourcePath": "/usr/src/app/logs"
            },
            "name": "AppLog"
        },
        {
            "host": {
                "sourcePath": "/tmp"
            },
            "name": "Tmp"
        }
    ]
}
